  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k \"nodemon\" \"ts-node-dev -r tsconfig-paths/register src/worker.ts\"",
    "build": "rimraf dist && tsc && tsc-alias && copy-folder src/templates dist/templates",
    "serve": "cross-env NODE_ENV=production concurrently -k \"node dist/index.js\" \"node dist/worker.js\"",
    "test": "jest",
    "gen:template": "ts-node scripts/gen-template.ts",
    "prisma:restart": "npm run prisma:migrate:reset && npm run prisma:push && npm run prisma:seed",
    "prisma:gen": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate:dev": "prisma migrate dev --skip-seed",
    "prisma:migrate:reset": "prisma migrate reset --skip-seed",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed"
  },
